ggerTo('.LZeLLPey_0, .LZeLLPey_1, .LZeLLPey_2, .LZeLLPey_3, .LZeLLPey_4', 1, {\r\n                    strokeDashoffset: 0,\r\n                    stagger: 1,\r\n                    ease: Power0.easeNone\r\n                })\r\n                .from('.pepe-logo .text', 5, {\r\n                    autoAlpha: 0,\r\n                    yPercent: -25,\r\n                    ease: Power3.easeOut\r\n                }, 0);\r\n\r\n            let sceneLogo = new ScrollMagic.Scene({\r\n                triggerElement: '.pepe-logo',\r\n                offset: -this.viewport.h / 3,\r\n                duration: this.viewport.h\r\n            })\r\n                .setTween(tlLogo)\r\n                .addTo(this.scroller)\r\n                .reverse(true);\r\n\r\n        },\r\n        beforeDestroy() {\r\n\r\n            this.scroller.destroy();\r\n\r\n        }\r\n    };\r\n</script>\r\n"],"sourceRoot":"src/views/work"}]}